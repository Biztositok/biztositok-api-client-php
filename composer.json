{
    "name": "biztositok/biztositok-api-client-php",
    "type": "library",
    "description": "SDK for the biztositok.hu API.",
    "keywords": [
        "biztositok.hu",
        "api client"
    ],
    "authors": [
        {
            "name": "Tamás Molnár",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Biztositok\\Api\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Biztositok\\Api\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "cs-check": "php-cs-fixer fix --config=.php_cs --dry-run --diff",
        "cs-fix": "php-cs-fixer fix --config=.php_cs"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
